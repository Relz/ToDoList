{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"noEmitOnError": true,
		"strictNullChecks": true,
		"skipLibCheck": true,
		"target": "es2017",
		"jsx": "react"
	},
	"include": [
		"**/*"
	],
	"exclude": [
		"node_modules"
	]
}
